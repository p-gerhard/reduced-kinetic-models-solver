void pn_num_flux_boundary_kinetic_R(const float wL[10], float flux[10])
{
	const float alpha = ALPHA;
	const float beta = BETA;
	const float x0 = alpha * wL[0];
	const float x1 = 8.94069672e-8F * beta;
	const float x2 = alpha * wL[2];
	const float x3 = -1.49011612e-8F * beta;
	const float x4 = alpha * wL[4];
	const float x5 = alpha * wL[5];
	const float x6 = alpha * wL[1];
	const float x7 = alpha * wL[3];
	const float x8 = alpha * wL[6];
	const float x9 = 0.0292317159F - 0.0292317159F * beta;
	const float x10 = alpha * wL[7];
	const float x11 = 0.0584633797F * beta - 0.0584633797F;
	const float x12 = alpha * wL[8];
	const float x13 = alpha * wL[9];
	const float x14 = 0.059761446F * beta;
	const float x15 = -0.0338291042F * beta;
	flux[0] = 0.250000089F * wL[0] + 0.288675129F * wL[2] -
			  0.0698771328F * wL[4] + 0.121030763F * wL[5] +
			  x0 * (-5.96046448e-8F * beta - 0.250000089F) +
			  x2 * (-x1 - 0.288675129F) + x4 * (x3 + 0.0698771179F) +
			  x5 * (x3 - 0.12103077F);
	flux[1] = 0.187500045F * wL[1] + 0.223606825F * wL[3] +
			  0.113213874F * wL[6] - 0.0292317178F * wL[7] + x10 * x9 +
			  x6 * (0.18750003F * beta - 0.18750003F) +
			  x7 * (0.223606795F * beta - 0.223606795F) +
			  x8 * (0.113213912F * beta - 0.113213912F);
	flux[2] = 0.288675129F * wL[0] + 0.374999911F * wL[2] -
