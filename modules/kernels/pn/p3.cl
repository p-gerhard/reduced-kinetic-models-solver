			  0.0698771328F * wL[4] - 0.12103074F * wL[5] -
			  0.250000089F * wR[0] + 0.288675129F * wR[1] +
			  0.0698771253F * wR[4] + 0.12103074F * wR[5];
	flux[1] =
		0.2886751F * wL[0] + 0.374999911F * wL[1] - 0.129099458F * wL[4] -
		0.223606855F * wL[5] - 0.0754759312F * wL[6] - 0.058463376F * wL[7] +
		0.288675129F * wR[0] - 0.374999911F * wR[1] - 0.129099458F * wR[4] -
		0.223606855F * wR[5] + 0.0754759237F * wR[6] + 0.0584633835F * wR[7];
