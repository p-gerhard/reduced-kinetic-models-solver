		0.2734375F * wL[5] + 0.231455013F * wL[6] + 0.0597614013F * wL[7] +
		0.12103074F * wR[0] - 0.223606855F * wR[1] - 0.0676582232F * wR[4] -
		0.2734375F * wR[5] + 0.231455028F * wR[6] + 0.0597613975F * wR[7];
	flux[6] = -0.0754759312F * wL[1] + 0.231455013F * wL[5] +
			  0.307617188F * wL[6] + 0.02647545F * wL[7] +
			  0.0754759237F * wR[1] + 0.231455028F * wR[5] -
			  0.307617188F * wR[6] - 0.0264754444F * wR[7];
	flux[7] = -0.058463376F * wL[1] + 0.207019672F * wL[4] +
			  0.0597614013F * wL[5] + 0.02647545F * wL[6] +
			  0.293945342F * wL[7] + 0.0584633835F * wR[1] +
			  0.207019672F * wR[4] + 0.0597613975F * wR[5] -
			  0.0264754444F * wR[6] - 0.293945342F * wR[7];
	flux[8] = -0.0292317159F * wL[2] - 0.0597614385F * wL[3] +
			  0.146972656F * wL[8] + 0.0397132225F * wL[9] +
