			  0.0292317159F * wL[8] - 0.113213919F * wL[9] -
			  0.18750006F * wR[2] + 0.22360681F * wR[3] +
			  0.0292317159F * wR[8] + 0.113213934F * wR[9];
	flux[3] = 0.22360681F * wL[2] + 0.31250003F * wL[3] -
			  0.0597614385F * wL[8] - 0.231455088F * wL[9] +
			  0.22360681F * wR[2] - 0.31250003F * wR[3] -
			  0.0597614385F * wR[8] - 0.231455103F * wR[9];
	flux[4] = -0.0698771328F * wL[0] - 0.129099458F * wL[1] +
			  0.1953125F * wL[4] + 0.0676582307F * wL[5] +
